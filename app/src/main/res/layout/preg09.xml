<RelativeLayout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/estadisticas"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:onClick="Salir"
    tools:context=".MainActivity" >

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true" >

        <LinearLayout
            android:id="@+id/lay9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="50dp"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/textPregunta9"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/PREGUNTA9"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textColor="#002266"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="15dp"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/textPregunta9a"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/PREGUNTA9a"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:textColor="#002266"
                        android:layout_marginBottom="15dp"
                        android:textStyle="bold" />

                    <EditText
                        android:id="@+id/editPregunta9a"
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:ems="10"
                        android:hint="AÃ±o"
                        android:inputType="number"
                        android:maxLength="4"
                        android:textSize="16sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="15dp"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/textPregunta9m"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/PREGUNTA9m"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:textColor="#002266"
                        android:layout_marginBottom="15dp"
                        android:textStyle="bold" />

                    <EditText
                        android:id="@+id/editPregunta9m"
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:ems="10"
                        android:hint="Mes"
                        android:inputType="number"
                        android:maxLength="2"
                        android:textSize="16sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="15dp"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/textPregunta9d"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/PREGUNTA9d"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:textColor="#002266"
                        android:layout_marginBottom="15dp"
                        android:textStyle="bold" />

                    <EditText
                        android:id="@+id/editPregunta9d"
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:ems="10"
                        android:hint="Dia"
                        android:inputType="number"
                        android:maxLength="2"
                        android:textSize="16sp" />
                </LinearLayout>



            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</RelativeLayout>
